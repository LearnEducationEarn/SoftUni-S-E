package Exam;

import java.util.Scanner;

public class P02_Pacman {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);

        int n = Integer.parseInt(scanner.nextLine());


        char[][] matrix = new char[n][n];


        char[][] grid = new char[n][n];
        int pacmanRow = 0;
        int pacmanCol = 0;
        int health = 100;
        int starsCount = 0;
        boolean immune = false;


        for (int i = 0; i < n; i++) {
            String row = scanner.nextLine();
            for (int j = 0; j < n; j++) {
                grid[i][j] = row.charAt(j);
                if (grid[i][j] == 'P') {
                    pacmanRow = i;
                    pacmanCol = j;
                } else if (grid[i][j] == '*') {
                    starsCount++;
                }
            }
        }

        // Process commands
        while (scanner.hasNextLine()) {
            String command = scanner.nextLine();
            if (command.equals("END")) {
                break;
            }

            // Mark current position as empty
            grid[pacmanRow][pacmanCol] = '-';

            // Move Pacman based on direction
            switch (command) {
                case "up":
                    pacmanRow = (pacmanRow - 1 + n) % n;
                    break;
                case "down":
                    pacmanRow = (pacmanRow + 1) % n;
                    break;
                case "left":
                    pacmanCol = (pacmanCol - 1 + n) % n;
                    break;
                case "right":
                    pacmanCol = (pacmanCol + 1) % n;
                    break;
            }

            // Check new position
            char newPos = grid[pacmanRow][pacmanCol];
            switch (newPos) {
                case '*':
                    starsCount--;
                    break;
                case 'G':
                    if (!immune) {
                        health -= 50;
                    }
                    immune = false; // remove immunity after encountering a ghost
                    break;
                case 'F':
                    immune = true;
                    break;
            }

            // Mark new position as Pacman's position
            grid[pacmanRow][pacmanCol] = 'P';

            if (health <= 0 || starsCount == 0) {
                break;
            }
        }

        // Output results
        if (health <= 0) {
            System.out.println("Game over! Pacman last coordinates [" + pacmanRow + "," + pacmanCol + "]");
        } else if (starsCount == 0) {
            System.out.println("Pacman wins! All the stars are collected.");
        } else {
            System.out.println("Pacman failed to collect all the stars.");
        }
        System.out.println("Health: " + health);
        if (starsCount > 0) {
            System.out.println("Uncollected stars: " + starsCount);
        }
        for (int i = 0; i < n; i++) {
            for (int j = 0; j < n; j++) {
                System.out.print(grid[i][j]);
            }
            System.out.println();
        }

        scanner.close();
    }}

