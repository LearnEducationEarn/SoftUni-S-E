package L08_Generics_Exercises.P11_Threeuple;

public class Tuple<U1, U2> {

        protected U1 key;
        protected U2 value;

        public Tuple(U1 key, U2 value) {
            this.key = key;
            this.value = value;
        }

        public U1 getKey() {
            return key;
        }

        public void setKey(U1 key) {
            this.key = key;
        }

        public U2 getValue() {
            return value;
        }

        public void setValue(U2 value) {
            this.value = value;
        }

        @Override
        public String toString() {
            return String.format("%s -> %s", this.key, this.value);
        }

}
