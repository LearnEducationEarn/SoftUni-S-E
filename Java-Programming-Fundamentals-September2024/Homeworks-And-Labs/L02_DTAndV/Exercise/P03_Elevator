package L02_DataTypesAndVariables_Exercise;

import java.util.Scanner;

public class P03_Elevator {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);

        short numberOfPeople = Short.parseShort(scanner.nextLine());
        byte capacity = Byte.parseByte(scanner.nextLine());

        double courses = (int) Math.ceil(numberOfPeople * 1.0 / capacity);
        System.out.printf("%.0f", courses);
    }
}


