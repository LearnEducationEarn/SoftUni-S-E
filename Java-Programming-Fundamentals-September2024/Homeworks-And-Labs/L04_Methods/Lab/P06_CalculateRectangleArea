package L04_Methods_Lab;
import java.util.Scanner;
public class P06_CalculateRectangleArea {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);

        byte width = Byte.parseByte(scanner.nextLine());
        byte length = Byte.parseByte(scanner.nextLine());
        System.out.println(returnAreaOfRectangle(width, length));
    }

    private static int returnAreaOfRectangle(byte width, byte length) {
        return width * length;
    }
}

