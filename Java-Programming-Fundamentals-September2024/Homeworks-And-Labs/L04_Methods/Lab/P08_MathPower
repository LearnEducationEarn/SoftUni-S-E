package L04_Methods_Lab;
import java.util.Scanner;
public class P08_MathPower {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);

        byte number = Byte.parseByte(scanner.nextLine());
        byte step = Byte.parseByte(scanner.nextLine());
        double result = getValueOfRaisedNumber(number, step);

        System.out.printf("%.0f", result);
    }

    private static double getValueOfRaisedNumber(byte number, byte step) {

        return Math.pow(number, step);
    }
}
