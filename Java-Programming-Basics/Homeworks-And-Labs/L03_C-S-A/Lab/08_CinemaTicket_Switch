
package L03_ConditionalStatementsAdvanced_Lab;

import java.util.Scanner;

public class P08_CinemaTicket_Switch {
    public static void main(String[] args) {

        Scanner scanner = new Scanner(System.in);


        String dayOfWeek = scanner.nextLine();
        int pricePerTicket = 0;
        switch (dayOfWeek) {
            case "Monday":
            case "Tuesday":
            case "Friday":
                pricePerTicket = 12;
                break;
            case "Wednesday":
            case "Thursday":
                pricePerTicket = 14;
                break;
            case "Saturday":
            case "Sunday":
                pricePerTicket = 16;
                break;

        }

        System.out.println(pricePerTicket);


    }
}
