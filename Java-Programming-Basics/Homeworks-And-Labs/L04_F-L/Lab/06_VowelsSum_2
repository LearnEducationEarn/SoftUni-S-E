
package L04_ForLoop_Lab;

import java.util.Scanner;

public class L06_VowelsSum_2 {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);

        String text = scanner.nextLine();

        double sumOfVowels = 0;

        for (int index = 0; index < text.length(); index++) {

            char currentSymbol = text.charAt(index);

            switch (currentSymbol) {

                case 'a':
                    sumOfVowels = sumOfVowels + 1;
                    break;
                case 'e':
                    sumOfVowels = sumOfVowels + 2;
                    break;
                case 'i':
                    sumOfVowels = sumOfVowels + 3;
                    break;
                case 'o':
                    sumOfVowels = sumOfVowels + 4;
                    break;
                case 'u':
                    sumOfVowels = sumOfVowels + 5;
                    break;


            }


        }

        System.out.printf("%.0f", sumOfVowels);


    }
}
